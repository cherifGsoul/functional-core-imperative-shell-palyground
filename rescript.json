{
  "gentypeconfig": {
    "module": "es6",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.ts",
    "importPath": "relative",
    "language": "typescript",
    "exportInterface": true,
    "shims": {
      "Js": "Js"
    },
    "debug": {
      "all": false
    }
  },
  "name": "your-project-name",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".res.js",
  "bs-dependencies": [
    "@rescript/core"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ]
}